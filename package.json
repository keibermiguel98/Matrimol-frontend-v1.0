{
  "name": "matrimol",
  "version": "1.0.0",
  "description": "Sistema de gestion de produccion",
  "main": "src/index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . matrimol --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron": "^21.2.0",
    "express": "^4.18.2",
    "html-pdf": "^3.0.1",
    "moment": "^2.29.4",
    "path": "^0.12.7",
    "promise-mysql": "^5.2.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "sweetalert2": "^11.7.3"
  },
  "devDependencies": {
    "electron-packager": "^17.1.1"
  }
}
